{
    "name": "ydg/hyperf-admin-generator",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "hyperf",
        "hyperf admin",
        "hyperf admin generator"
    ],
    "authors": [
        {
            "name": "ydg",
            "email": "1028180290@qq.com",
            "role": "Developer"
        }
    ],
    "description": "hyperf admin code generator",
    "autoload": {
        "psr-4": {
            "HyperfAdminGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "HyperfAdminGenerator\\ConfigProvider"
        }
    }
}
